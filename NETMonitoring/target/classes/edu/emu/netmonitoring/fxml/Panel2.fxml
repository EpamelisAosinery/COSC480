<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="edu.emu.netmonitoring.controller.Panel2Controller">
    <children>
        <Label text="Network Information">
            <font>
                <Font name="System Bold" size="40"/>
            </font>
        </Label>
        <TableView fx:id="tableView" layoutX="86.0" layoutY="92.0" prefHeight="250.0" prefWidth="502.0">
            <columns>

                <TableColumn fx:id="cInterfaceName" prefWidth="100.0" text="Interface Name"/>
                <TableColumn fx:id="cDisplayName" prefWidth="200.0" text="Display Name"/>
                <TableColumn fx:id="cIPAddress" prefWidth="150.0" text="IP Address"/>
                <TableColumn fx:id="cStatus" prefWidth="50.0" text="Status"/>
            </columns>
        </TableView>
    </children>
</Pane>
